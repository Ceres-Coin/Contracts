        _;
    }

    // ------------------------------------------------------------------------
    // Setters
    // ------------------------------------------------------------------------
    function setStakingAddress(address tokenAddress, address _stakingAddress) public onlyGovernance { // TODO Owner
        stakingAddress[tokenAddress] = _stakingAddress;
    }

